{
  "name": "electron-browser-ai",
  "version": "1.0.0",
  "description": "基于Electron的浏览器应用，具有AI生成页面使用说明功能",
  "main": "main/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder",
    "start:debug": "electron . --enable-logging --remote-debugging-port=9222"
  },
  "keywords": [
    "electron",
    "browser",
    "ai",
    "automation",
    "mcp"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.3.3",
    "electron-builder": "^24.0.0"
  },
  "dependencies": {
    "axios": "^1.11.0"
  },
  "build": {
    "appId": "com.example.electron-browser-ai",
    "productName": "Electron Browser AI",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main/**/*",
      "renderer/**/*",
      "package.json"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    }
  }
}